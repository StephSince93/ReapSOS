<ion-header>

  <ion-navbar>
    <ion-title>Mileage</ion-title>
  </ion-navbar>

</ion-header>


<ion-content>
  <form #f="ngForm" (ngSubmit)="onSubmit(f)">
    <ion-list>
      <ion-item class="formField">
        <ion-label color="primary" stacked>Number of Trips</ion-label>
        <ion-input
        type="number"
        name="trips"
        ngModel></ion-input>
      </ion-item>

      <ion-item class="formField">
        <ion-label color="primary" stacked>Number of Miles</ion-label>
        <ion-input
        ngModel
        type="number"
        name="miles">
        </ion-input>
      </ion-item>

      <ion-item>
        <button id="submitButton" ion-button full large>Submit</button>
     </ion-item>
   </ion-list>
  </form>
</ion-content>

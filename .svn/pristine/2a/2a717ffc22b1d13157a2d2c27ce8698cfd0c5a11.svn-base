iProvider } from '../../providers/stem-api/stem-api';\nimport { NgForm } from '@angular/forms';\n\nimport { MenuPage } from '../menu/menu';\n\n@Component({\n  selector: 'page-login',\n  templateUrl: 'login.html'\n})\nexport class LoginPage {\n\n\n    res:any = {};\n    authToken = {\"AuthToken\":\"\"};\n    static initialLogin:boolean = false;\n    constructor(public navCtrl: NavController,\n                public navParams: NavParams,\n                public stemAPI: StemApiProvider,\n                private loadingCtrl: LoadingController,\n                private alertCtrl: AlertController) {\n\n              }\n\n              onSubmit(form: NgForm){\n            const loading = this.loadingCtrl.create({\n              content: 'Signing in...'\n            });\n            loading.present();\n            //console.log(form.value);\n\n            // API POST authentication\n            this.stemAPI.validateUser(form.value).then((result) =>{\n              //clears values of the form after data is saved to array\n              form.reset();\n              //converts result to array\n              this.res = JSON.parse(result.toString());\n              //console.log(this.res);\n              //sets authtoken to local storage\n              if(this.res.token!=\"\"){\n              localStorage.setItem('authToken',this.res.token)\n                  }\n              //console.log(localStorage);\n              if(this.res.status == true){\n               setTimeout(() => {\n                LoginPage.initialLogin = true;\n                this.navCtrl.push(MenuPage);\n                 loading.dismiss();\n              }, 1000);\n            }\n              else{\n                  loading.dismiss();\n                const alert = this.alertCtrl.create({\n                  title: this.res.msg,\n                  buttons: ['Dismiss']\n                });\n                alert.present();\n              }\n               //console.log(result.toString());\n            }, (err) => {\n                  loading.dismiss();\n                const alert = this.alertCtrl.create({\n                  title: err.msg,\n                  buttons: ['Dismiss']\n                });\n                alert.present();\n                console.log(err);\n            });\n           }\n          }\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/login/login.ts"],"sourceRoot":""}